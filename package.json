{
  "name": "vite-reactts-eslint-prettier",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm-run-all dev:vite",
    "dev:vite": "vite",
    "build": "yarn run lint && tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,scss,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc"
  },
  "dependencies": {
    "@headlessui/react": "^1.0.0",
    "@types/moment": "^2.13.0",
    "@types/react-color": "^3.0.4",
    "autoprefixer": "^10.2.5",
    "chokidar-cli": "^2.1.0",
    "cookie": "^0.4.1",
    "faker": "^5.5.3",
    "formik": "^2.2.6",
    "i18next": "^20.2.2",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-cookie": "^4.0.3",
    "react-custom-checkbox": "^1.0.7",
    "react-dom": "^17.0.0",
    "react-hot-toast": "^1.0.2",
    "react-i18next": "^9.0.10",
    "react-input-slider": "^6.0.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.0",
    "react-tooltip": "^4.2.17",
    "sass": "^1.32.8",
    "styled-component": "^2.8.0",
    "styled-components": "^5.2.3",
    "tailwindcss": "^2.1.1",
    "use-clipboard-copy": "^0.2.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/faker": "^5.5.4",
    "@types/node-sass": "^4.11.1",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.5",
    "@types/react-cookies": "^0.1.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2",
    "vite": "^2.1.0"
  },
  "pre-commit": "lint",
  "license": "MIT"
}
